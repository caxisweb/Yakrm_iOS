	<array>
		<string>whatsapp</string>
		<string>twitter</string>
		<string>fb</string>
		<string>com.inforaam.Yakrm.payments</string>
	</array>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<key>NSCameraUsageDescription</key>
	<string>Yakrm App Camera requires access to read barcode number</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Yakrm App Camera requires to Upload Video</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Yakrm App Photos requires to Upload Profile Image</string>
