	<array>
		<string>ArbFONTS-GE_SS_Two_Bold.otf</string>
		<string>ArbFONTS-GE_SS_Two_Light.otf</string>
		<string>ArbFONTS-GE_SS_Two_Medium.otf</string>
	</array>
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
	</array>
